<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>

<AnchorPane prefHeight="120.0" prefWidth="200.0" xmlns="http://javafx.com/javafx/11.0.14-internal" xmlns:fx="http://javafx.com/fxml/1" fx:controller="root.intelligentasteroidsshooter.RestartViewController">
   <children>
      <Button fx:id="yesButton" layoutX="14.0" layoutY="80.0" mnemonicParsing="false" onAction="#onYesButtonClick" prefWidth="80.0" text="Yes" />
      <Button fx:id="closeButton" layoutX="106.0" layoutY="80.0" mnemonicParsing="false" onAction="#onCloseButtonClick" prefWidth="80.0" text="Close Game" />
      <VBox prefHeight="77.0" prefWidth="200.0" alignment="CENTER">
         <children>
            <Label fx:id="scoreText">
                 <font>
                     <Font size="18.0" />
                 </font>
             </Label>
      
            <Label fx:id="question" text="Play again?">
                 <font>
                     <Font size="18.0" />
                 </font>
            </Label>
         </children>
      </VBox>
   </children>
</AnchorPane>
